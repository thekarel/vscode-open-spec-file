{
    "name": "open-spec-file",
    "displayName": "Open Spec File",
    "description": "Quickly access spec files for TDD zen",
    "version": "0.0.1",
    "publisher": "thekarel",
    "engines": {
        "vscode": "^1.13.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.openSpecFile"
    ],
    "main": "./src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.openSpecFile",
            "title": "Open Spec File"
        }],
        "keybindings": [{
            "command": "extension.openSpecFile",
            "key": "ctrl+k ctrl+k",
            "mac": "cmd+k cmd+k"
        }]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "eslint": "^3.6.0",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}